{
  "name": "@analytodon/cli",
  "description": "Analytodon CLI Tools",
  "version": "0.0.0",
  "author": "Raphael StÃ¤bler",
  "bin": {
    "analytodon-cli": "./bin/run.js"
  },
  "bugs": "https://github.com/blazer82/analytodon/issues",
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6.2.37",
    "@oclif/plugin-plugins": "^5.4.56",
    "axios": "^1.13.5",
    "dotenv": "^16.6.1",
    "megalodon": "^10.2.3",
    "mongodb": "^6.21.0",
    "pino": "^9.14.0",
    "pino-pretty": "^13.1.3",
    "timezone-support": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4.1.16",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.130",
    "chai": "^4.5.0",
    "eslint": "^9.39.2",
    "mocha": "^10.8.2",
    "oclif": "^4.22.79",
    "prettier": "^3.8.1",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/blazer82/analytodon",
  "keywords": [
    "oclif"
  ],
  "license": "GPL-3.0-only",
  "main": "dist/index.js",
  "oclif": {
    "bin": "analytodon-cli",
    "dirname": "analytodon-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "blazer82/analytodon",
  "scripts": {
    "build": "shx rm -rf dist && shx rm -f tsconfig.tsbuildinfo && tsc -b",
    "prettier:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "prettier:write": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "analytodon-cli": "node bin/run.js"
  },
  "types": "dist/index.d.ts"
}
